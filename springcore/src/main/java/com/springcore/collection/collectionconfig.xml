			<list>
				<value>1264764287</value>
				<value>2375774778</value>
				<value>3475634785</value>
			</list>
		</property>
		<property name="Address">
			<set>
				<value>delhi</value>
				<value>pune</value>
				<value>mumbai</value>
			</set>
		</property>
		<property name="coureses">
			<map>
				<entry key="java" value="100/"/>
				<entry key="spring" value="500"/>
				<entry key="python" value="1000"/>
			</map>
